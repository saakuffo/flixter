<br />
<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1 class="text-center"><%= @course.title%></h1>
  <br />
  <%= link_to 'Preview as a Student', course_path(@course), class: 'btn btn-warning pull-right' %>
  <br clear="clr">

  <div class="text-center">
    <%= link_to 'Add a new sections...', new_instructor_course_section_path(@course), class: 'btn btn-primary btn-lg' %>
  </div>
  <ul>
    <% @course.sections.each do |section| %>
      <li>
        <%= section.title %>
      </li>
    <% end %>
  </ul>
</div>